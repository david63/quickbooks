{
	"name": "david63/quickbooks",
	"type": "phpbb-extension",
	"description": "An extension to add bookmarks to the Quick Links menu.",
	"version": "3.1.0-RC1",
	"time": "2020-08-16",
	"keywords": [
		"phpbb",
		"extension",
		"bookmarks",
		"menu",
		"quick links"
	],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "David Wood",
			"role": "Developer"
		}
	],
   "require": {
        "php": ">=7.1.3",
        "composer/installers": "~1.0"
    },
	"extra": {
		"display-name": "Quick link bookmarks",
		"soft-require": {
			"phpbb/phpbb": ">=3.3.0,<4.0.0@dev"
		},
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/versions",
        	"filename": "quick_books_version_file.json",
			"ssl": false
    	}
	}
}
